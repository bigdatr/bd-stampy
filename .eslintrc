{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jasmine"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "strict": 0,
    "key-spacing": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "quotes": 0,
    "new-cap": 0,
    "no-multi-spaces": 0,
    "camelcase": 0,
    "no-alert": 0,
    "no-underscore-dangle": 0
  }
}